/*2013*/
SELECT
  '2013' as schendyr
  ,stu.state_student_id
  ,ela13."cst_2013_ela_testGradeLevel" as grade
  ,'ELA' as groupname
  ,ela13."cst_2013_ela_testTakenText" as testname
  ,ela13."cst_2013_ela_scaleScore" as scalescore
  ,ela13."cst_2013_ela_performanceLevelText" as proficiencylevel

FROM state_data_ca.cst_2013_ela ela13
LEFT JOIN public.students stu on stu.student_id=ela13.student_id

UNION

SELECT
  '2013' as schendyr
  ,stu.state_student_id
  ,math13."cst_2013_math_testGradeLevel" as grade
  ,'Math' as groupname
  ,math13."cst_2013_math_testTakenText" as testname
  ,math13."cst_2013_math_scaleScore" as scalescore
  ,math13."cst_2013_math_performanceLevelText" as proficiencylevel

FROM state_data_ca.cst_2013_math math13
LEFT JOIN public.students stu on stu.student_id=math13.student_id

UNION

SELECT
  '2013' as schendyr
  ,stu.state_student_id
  ,sci13."cst_2013_sci_testGradeLevel" as grade
  ,'Science' as groupname
  ,sci13."cst_2013_sci_testTakenText" as testname
  ,sci13."cst_2013_sci_scaleScore" as scalescore
  ,sci13."cst_2013_sci_performanceLevelText" as proficiencylevel

FROM state_data_ca.cst_2013_sci sci13
LEFT JOIN public.students stu on stu.student_id=sci13.student_id

UNION

SELECT
  '2013' as schendyr
  ,stu.state_student_id
  ,hss13."cst_2013_hss_testGradeLevel" as grade
  ,'Social Science' as groupname
  ,hss13."cst_2013_hss_testTakenText" as testname
  ,hss13."cst_2013_hss_scaleScore" as scalescore
  ,hss13."cst_2013_hss_performanceLevelText" as proficiencylevel

FROM state_data_ca.cst_2013_hss hss13
LEFT JOIN public.students stu on stu.student_id=hss13.student_id

